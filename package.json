{
  "name": "homematic-manager",
  "version": "2.0.0-beta.5",
  "private": true,
  "dependencies": {
    "async": "2.6.0",
    "binrpc": "3.1.2",
    "electron-ipc-rpc": "1.1.0",
    "electron-is-dev": "0.3.0",
    "electron-unhandled": "^1.1.0",
    "electron-window-state": "4.1.1",
    "free-jqgrid": "4.15.4",
    "hm-discover": "1.0.0",
    "homematic-xmlrpc": "1.0.2",
    "iconv-lite": "0.4.19",
    "jquery": "3.3.1",
    "jquery-ui-dist": "1.12.1",
    "jquery-ui-multiselect-widget": "2.0.2",
    "nextport": "1.0.0",
    "persist-json": "1.2.0",
    "request": "2.85.0",
    "ui-contextmenu": "1.18.1",
    "yalm": "4.1.0"
  },
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "xo": "latest"
  },
  "description": "Webinterface for HomeMatic interface processes (rfd, hs485d, Homegear)",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dist": "build",
    "lint": "xo",
    "lintfix": "xo --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hobbyquaker/homematic-manager"
  },
  "keywords": [
    "HomeMatic",
    "BidCoS",
    "eQ-3",
    "rfd",
    "hs485d",
    "Homegear",
    "XML-RPC",
    "Smarthome",
    "Internet of Things",
    "IoT"
  ],
  "author": "Sebastian 'hobbyquaker' Raff <hobbyquaker@gmail.com>",
  "contributors": [
    "Andr√© 'Anli' Litfin"
  ],
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://github.com/hobbyquaker/homematic-manager/issues"
  },
  "homepage": "https://github.com/hobbyquaker/homematic-manager",
  "build": {
    "productName": "Homematic Manager",
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "tar.gz"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    }
  },
  "xo": {
    "space": 4,
    "ignore": [
      "tools/convert_easymodes.js",
      "tools/convert_imgpaths.js"
    ]
  }
}
